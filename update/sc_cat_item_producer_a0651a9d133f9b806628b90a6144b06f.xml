        <script><![CDATA[var caseGR = new GlideRecord('x_snc_lend_case');
if ( caseGR.get( producer.loan_case ) ) {
	caseGR.entry_stage = new LoanStageMgmt().getStep('income');
	caseGR.update();
	
	new LoanRecordMgmt().createFromSet('rpvs_b_current_employment', 'x_snc_lend_employment', 'b', producer, {
		borrower: caseGR.borrower,
		current: true
	});
	
	new LoanRecordMgmt().createEachFromSet('rpvs_h_employment_history', 'x_snc_lend_employment', 'h', producer, {
			borrower: caseGR.borrower
		});
	
	if (caseGR.co_borrower) {
		new LoanRecordMgmt().createFromSet('rpvs_cb_current_employment', 'x_snc_lend_employment', 'cb', producer, {
			borrower: caseGR.co_borrower,
			current: true
		});
		
		new LoanRecordMgmt().createEachFromSet('rpvs_cbh_employment_history', 'x_snc_lend_employment', 'cbh', producer, {
			borrower: caseGR.co_borrower
		});
		
	}
}

current.setAbortAction(true);

producer.portal_redirect = "?id=loan_application&case="+producer.loan_case+"&spa=1";
producer.url_redirect =  producer.portal_redirect;]]></script>
